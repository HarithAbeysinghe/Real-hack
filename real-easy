n = int(input("Number of Marbles :"))
values = list(map(int, input("Enter Multiple Values: ").split()))
price = 0
for j in range(0,n):
    price += (n-j)*2**(j)
print(price)




m, n = input().split()
m, n = int(m), int(n)
numbers = 0
for i in range(m,n+1):
    facts = 0
    for j in range(1,i+1):
        if i%j ==0:
            facts += 1
            if facts >2 and facts%2 ==0:
                numbers+=1
print(numbers)
